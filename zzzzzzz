<script>
    $(function () {
        $(".shareDia").click(function () {
            $(this).css("display", "none");
        });
        $(".openDialog").click(function () {
            window.history.go(-1);
        });
        $(".shareBtn").click(function () {
            $(".shareDia").css("display", "");
        });
    })
    function hid() {
        $(".shareDia").css("display", "");
    };

    $(document).ready(function () {
        $("#open-clicks").click(function () {
            $(".inv-new-page").css({ display: "table" });
        });
        $("#close-clicks").click(function () {
            $(".inv-new-page").css({ display: "none" });
        });
    });


    function getQueryString(name) {

        var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");

        var r = window.location.search.substr(1).match(reg);

        if (r != null) return unescape(r[2]); return null;

    }

    function openapp(appid) {
        if (appid == "pengyou") {
            appid = "type=100||sharetype=2";
        }
        if (appid == "weixin") {
            appid = "type=100||sharetype=1";
        }
        else if (appid == "account") {
            appid = "type=18||";
        }
        var ostype = getQueryString("ostype");

        if (ostype == "1") {
            appid = "md://iphoneapp?" + appid;

            $("body").append("<iframe id='appiframe' src='" + appid + "' style='display: none;'></iframe>");

        } else if (ostype == null || ostype == "") {

            window.phone.startWebActivity(appid);
            
        }

    }


    $(document).ready(function () {
        $("#open-clicks1").click(function () {
            $(".inv-new-page").css({ display: "table" });
        });
        $("#close-clicks1").click(function () {
            $(".inv-new-page").css({ display: "none" });
        });
    });


    $(document).ready(function () {
        $(".lookGift").click(function () {
            $(".inv-new-pagee").css({ display: "table" });
        });
        $("#close-clickS").click(function () {
            $(".inv-new-pagee").css({ display: "none" });
        });
    });


</script>








         
    
          
